<plist version="1.0"><dict>
<key>Label</key><string>com.mavops.activityagent</string>
<key>ProgramArguments</key><array><string>/usr/bin/env</string><string>python3</string><string>/Users/USERNAME/path/to/mac_agent/main.py</string></array>
